{
  "manifest_version": 3,
  "name": "Chromium Dark Tab Load",
  "version": "1.1",
  "description": "Eliminates white flash on page load",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
